        pagination_separator="#d2d2d2"
        mirror_border_color="#d2d2d2"
        mirror_border_width="2.0"
        clipped="false"
        />
        
        <video id="tip_video" value="tipsregistro.mp4"/>
        
        <text id="tip"
        color="#7f7f7f"
        font="CircularStd-Bold.otf"
        font_size="27.0"
        />
        
        <text id="tip_detail"
        color="#7f7f7f"
        font="CircularStd-Regular.otf"
        font_size="17.0"
        />
        
        <button id="button_back"
        background="#FFFFFF"
        decorator="#FFFFFF"
        foreground="#7f7f7f"
        content="arrow-back-dark.png"
        content_type="RESOURCE_ID"
        font=""
        font_size=""
        align="LEFT"
        />
        
        <button id="button_done"
        background="#FFFFFF"
        decorator="#FFFFFF"
        foreground="#7f7f7f"
        content="button_ready"
        content_type="TEXT_ID"
        font="CircularStd-Bold.otf"
        font_size="14.0"
        align="RIGHT"
        />
        
        <buttonImage id="button_exit" value="cross-dark.png"/>
        
        <image id="pagination_on" value="paged-on-dark.png"/>
        <image id="pagination_off" value="paged-off-dark.png"/>
    </view>
    <view id="StartExtractor">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        mirror_mist_color="#FFFFFF80"
        clipped="true"
        />
        
        <text id="text"
        color="#939298"
        font="CircularStd-Regular.otf"
        font_size="17.0"
        line_height="20.0"
        valign="CENTER"
        />
        
        <camera id="camera" />
        
        <button id="button_start"
        background="#49317d"
        decorator="#49317d"
        foreground="#ffffff"
        content="button_start"
        content_type="TEXT_ID"
        font="CircularStd-Regular.otf"
        font_size="18.0"
        align="CENTER"

        />
        
        <buttonImage id="button_info" value="info-dark.png"/>
        
        <buttonImage id="button_exit" value="cross-dark.png"
        />
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="WaitRecording">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="false"
        />
        
        <buttonImage id="button_exit" value="cross-dark.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="Extractor">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="true"
        />
        
        <camera id="camera" />
        
        <video id="extraction_video" value="tipsregistro.mp4" mode="Normal"/>
        
        <text id="text"
        color="#939298"
        font="CircularStd-Regular.otf"
        font_size="17.0"
        mode="Normal"
        valign="CENTER"
        />
        
        <text id="warning"
        color="#ff5b55"
        font="CircularStd-Regular.otf"
        font_size="17.0"
        mode="Warning"
        valign="CENTER"
        />
        
        <buttonImage id="button_exit" value="cross-dark.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="LivenessMove">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="true"
        />
        
        <camera id="camera" />
        
        <animation id="liveness_move" name="ArrowProgressExpulsion_" ext="png" start="01" end="36"/>
        <waiting id="waiting" color="#939298"/>
        
        <text id="liveness_move_text"
        color="#939298"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        mode="Normal"
        />
        
        <text id="warning"
        color="#ff5b55"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        mode="Warning"
        />
        
        <progress id="progress"
        background_color="#e9ecef"
        progress_color="#86449a"
        />
        
        <buttonImage id="button_exit" value="cross-dark.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="ScanQR">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        />
        
        <camera id="camera" />
        
        <text id="text"
        color="#939298"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        mode="Normal"
        />
        
        <text id="warning"
        color="#ff5b55"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        mode="Warning"
        />
        
        <fade id="qr_fade" target_color="#FFFFFF"/>
        
        <buttonImage id="button_exit" value="cross-dark.png"/>
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="Results">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="false"
        />
        
        <text id="tip"
        color="#939298"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        line_height="20.0"
        />
        
        <results id="results"
        background_color="#f9f9f9"
        fontResult="CircularStd-Regular.otf"
        fontResult_size="30.0"
        fontQuality="CircularStd-Bold.otf"
        fontQuality_size="17.0"
        />
        
        <button id="button_repeat"
        background="#49317d"
        decorator="#49317d"
        foreground="#ffffff"
        content="button_repeat"
        content_type="TEXT_ID"
        font="CircularStd-Bold.otf"
        font_size="18.0"
        align="CENTER"
        />
        
        <button id="button_finish"
        background="#ff3366"
        decorator="#ff3366"
        foreground="#ffffff"
        content="button_finish"
        content_type="TEXT_ID"
        font="CircularStd-Bold.otf"
        font_size="18.0"
        align="CENTER"
        />
        
        <image id="logo_medium" value="logo-medium.png"/>
    </view>
    <view id="Success">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="false"
        />
        
        <video id="video_success" value="checkExitoSobreGris.mp4"/>
        
        <image id="logo_large" value="logo-large.png"/>
    </view>
    <view id="Error">
        <background id="background"
        top="#FFFFFF"
        middle_top="#FFFFFF"
        middle_bottom="#FFFFFF"
        bottom="#FFFFFF"
        clipped="false"
        />
        
        <button id="button_error"
        background="#ff3366"
        decorator="#ff3366"
        foreground="#ffffff"
        content="button_finish"
        content_type="TEXT_ID"
        font="CircularStd-Bold.otf"
        font_size="14.0"
        align="CENTER"
        />

        <text id="text_error"
        color="#a4adb8"
        font="CircularStd-Bold.otf"
        font_size="17.0"
        />
        <imageError id="imageError"/>
        
        <image id="logo_large" value="logo-large.png"/>
    </view>
